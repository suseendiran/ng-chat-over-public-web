{
  "hosting": [
    {
      "site": "ng-chat-web",
      "public": "dist/apps/chat",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "inject-chat-script",
      "public": "chat-inject-script",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    }
  ]
}
